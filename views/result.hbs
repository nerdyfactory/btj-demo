<div class="container" style="margin-top: 2%">
  <div class="row">
    <div class="four columns">
      <label>1. Original</label>
      <img class=u-full-width src=/images/output/{{filename}}1.png>
      </img>
    </div>
    <div class="four columns">
      <label>2. HSV color space</label>
      <img class=u-full-width src=/images/output/{{filename}}2.png>
      </img>
    </div>
    <div class="four columns">
      <label>3. Hue from HSV</label>
      <img class=u-full-width src=/images/output/{{filename}}3.png>
      </img>
    </div>
  </div>
  <div class="row">
    <div class="four columns">
      <label>4. Otsu’s Binarization</label>
      <img class=u-full-width src=/images/output/{{filename}}4.png>
      </img>
    </div>
    <div class="four columns">
      <label>5. Contours</label>
      <img class=u-full-width src=/images/output/{{filename}}5.png>
      </img>
    </div>
    <div class="four columns">
      <label>6. Final</label>
      <img class=u-full-width src=/images/output/{{filename}}6.png>
      </img>
    </div>
  </div>
  <div class="row">
    <div class="two columns">
      <label>팔기장</label>
      <p>{{size.arm}}</p>
    </div>
    <div class="two columns">
      <label>가슴둘레</label>
      <p>{{size.chest}}</p>
    </div>
    <div class="two columns">
      <label>총기장</label>
      <p>{{size.height}}</p>
    </div>
  </div>
</div>
